<?xml version="1.0"?>
<info>
	<id>opencloudmesh</id>
	<name>Open Cloud Mesh</name>
	<namespace>OpenCloudMesh</namespace>
	<summary>Supports federated sharing with groups</summary>
	<description>The Open Cloud Mesh protocol specification prescribes support for shares to a user or to a group, but the ownCloud core implementation only supports the share type "user". This app fills in the missing share type "group".
	</description>
	<website>https://github.com/pondersource/oc-opencloudmesh</website>
	<bugs>https://github.com/pondersource/oc-opencloudmesh/issues</bugs>
	<repository type="git">https://github.com/pondersource/oc-opencloudmesh.git</repository>
	<licence>agpl</licence>
	<author>Developed for SURF by Ponder Source</author>
    <version>0.1.1</version>
	<documentation>
		<user>https://github.com/pondersource/oc-opencloudmesh</user>
		<admin>https://github.com/pondersource/oc-opencloudmesh</admin>
	</documentation>
	<category>collaboration</category>
	<types>
		<authentication/>
		<dav/>
	</types>
	<use-migrations>true</use-migrations>
	<screenshot>https://user-images.githubusercontent.com/123634558/247931425-3ea154a3-d2e0-49cb-9366-6c871ebfafcb.png</screenshot>
	<background-jobs>
		<job>OCA\OpenCloudMesh\Files_Sharing\External\ScanExternalSharesJob</job>
	</background-jobs>
	<dependencies>
		<owncloud min-version="10.4" max-version="10" />
	</dependencies>
</info>
